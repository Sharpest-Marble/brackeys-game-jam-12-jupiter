size_flags_vertical = 3
size_flags_stretch_ratio = 5.0
theme_override_font_sizes/font_size = 30
text = "The Building of an Ark is accomplished by building six Hibernation Chambers in the Upwelling room. These chambers are very expensive, and require huge supplies of food to keep our people alive in suspended animation for the 500 years the storm will rage."
vertical_alignment = 1
autowrap_mode = 2

